{
  "compilerOptions": { 
    "target": "es2016", 
    "module": "commonjs", 
    "rootDir": "./src", 
    "baseUrl": "src", 
    // "rootDirs": [], 
    "outDir": "./dist", 
    "noEmitOnError": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true,     
    "skipLibCheck": true, 
    "paths": { 
      "#Models/*": ["Model/*"], 
      "#Utils/*": ["utils/*"],
      "#Types/*": ["@types/*"],
      "#ErrorHandlers/*": ["Errors/*"],
      "#Auth/*": ["Auth/*"],
    }, 
  },
  "include": [
    "src/**/*.ts", 
  ],
  "exclude": [
    "node_modules",
    "dist",
  ],
}
